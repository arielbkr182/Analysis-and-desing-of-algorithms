Algoritmo consumo_gasol
	escribir"ingresa el tiempo en horas que duro el viaje ";
	Leer a;
	Escribir "ingresa la velocidad promedio durante el viaje";
	Leer b;
	multiplicacion<-(a*b);
	division<-(multiplicacion/12);
